
@using System.Data;
@{
    DataTable dt = ViewBag.list as DataTable;
}
<table class="table table-bordered">
    <tr>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>订单号</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>出单日期</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>车牌号</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>被保险人</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>承保公司</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>交强险保单号</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>商业险保单号</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>商业险平台费率</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>交强险平台费率</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>交强险保险费</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>商业险保险费</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>车船税保险费</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>佣金</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>总价格</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>返现</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>实付价格</strong></td>
        <td align="center" nowrap="nowrap" bgcolor="#f1f1f1"><strong>运费</strong></td>

    </tr>
    @if (dt.Rows.Count > 0)
    {
        foreach (DataRow item in dt.Rows)
        {
            <tr>
                <td align="center">@item["OrderCode"]</td>
                <td align="center">@item["CreateDate"]</td>
                <td align="center">@item["LicenseNo"]</td>
                <td align="center">@item["Name"]</td>
                <td align="center">@item["ProductName"]</td>
                <td align="center">@item["ForceNo"]</td>
                <td align="center">@item["BizNo"]</td>
                <td align="center">@Convert.ToDecimal(item["BizRate_Channel"]).ToString("F2")</td>
                <td align="center">@Convert.ToDecimal(item["ForceRate_Channel"]).ToString("F2")</td>
                <td align="center">@Convert.ToDecimal(item["ForceTotal"]).ToString("F2")</td>
                <td align="center">@Convert.ToDecimal(item["BizTotal"]).ToString("F2")</td>
                <td align="center">@Convert.ToDecimal(item["TaxTotal"]).ToString("F2")</td>
                <td align="center">@Convert.ToDecimal(item["Commission"]).ToString("F2")</td>
                <td align="center">@Convert.ToDecimal(item["Total"]).ToString("F2")</td>
                <td align="center">@Convert.ToDecimal(item["BackMoney"]).ToString("F2")</td>
                <td align="center">@((Convert.ToDecimal(item["Total"]) - Convert.ToDecimal(item["BackMoney"])).ToString("F2"))</td>
                <td align="center">
                    @item["DeliverPrice"]
                </td>
            </tr>
        }
    }
    else
    {
        <tr>
            <td align="center" style="color:red;" colspan="15">未查询到相关数据！</td>
            

        </tr>
    }

</table>