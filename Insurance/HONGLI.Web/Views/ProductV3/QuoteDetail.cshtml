        //标识调用次数  loading只调两次
        var requestFlag = 0;

        //获取报价信息
        function getSpecialPrecisePrice(submitStatus) {
            $.ajax({
                type: "post",
                url: '@Url.Action("GetSpecialPrecisePrice", "ProductV3")',
                dataType: "json",
                data: {
                    channel: "@ViewBag.channel",
                    mobile: "@ViewBag.mobile",
                    licenseNo: "@ViewBag.licenseNo",
                    intentionCompany:  "@ViewBag.intentionCompany",
                    userID:"@Request["userID"]",
                ProductItemId:"@Request["ProductItemId"]"
                },
                beforeSend: function () {
                    layer.open({ type:2 });
                },
                success: function (data) {
                    console.log(data);
                    if(data ==null){
                        alert("获取报价信息失败！");
                        return;
                    }
                    if (data.BusinessStatus != 1) {
                        var message = data.StatusMessage;
                        if (message == null) {
                            message = "获取报价信息失败，服务器未返回失败原因！";
                        }
                        alert(message);
                    }
                    if (data.Item == null) {
                        alert("没有获取到报价信息！");
                        return;
                    }

                    var result = data.Item;
                    var forceTotal = result.ForceTotal;
                    var taxTotal = result.TaxTotal;
                    var bizTotal = result.BizTotal;
                    var total = (forceTotal + taxTotal + bizTotal);
                    var totalFormat = common.operateNum.formatNum(total, 2);
                    $("#total").removeClass("t-loading").text(totalFormat);
                    $("#forceTotal").removeClass("t-loading").text(forceTotal);
                    $("#taxTotal").removeClass("t-loading").text(taxTotal);
                    $("#bizTotal").removeClass("t-loading").text(bizTotal);
                    $("#description").removeClass("t-loading").text(result.Description);

                    showItem($("#chesun"),result.CheSun.BaoE,result.CheSun.BaoFei);
                    showItem($("#bujimianchesun"),result.BuJiMianCheSun.BaoE,result.BuJiMianCheSun.BaoFei);
                    showItem($("#sanzhe"),result.SanZhe.BaoE,result.SanZhe.BaoFei);
                    showItem($("#bujimiansanzhe"),result.BuJiMianSanZhe.BaoE,result.BuJiMianSanZhe.BaoFei);
                    showItem($("#siji"),result.SiJi.BaoE,result.SiJi.BaoFei);
                    showItem($("#chengke"),result.ChengKe.BaoE,result.ChengKe.BaoFei);
                    showItem($("#daoqiang"),result.DaoQiang.BaoE,result.DaoQiang.BaoFei);
                    showItem($("#bujimiandaoqiang"),result.BuJiMianDaoQiang.BaoE,result.BuJiMianDaoQiang.BaoFei);
                    showItem($("#huahen"),result.HuaHen.BaoE,result.HuaHen.BaoFei);
                    showItem($("#boli"),result.BoLi.BaoE,result.BoLi.BaoFei,"boli");
                    showItem($("#sheshui"),result.SheShui.BaoE,result.SheShui.BaoFei);
                    showItem($("#ziran"),result.ZiRan.BaoE,result.ZiRan.BaoFei);
                    showItem($("#chedeng"),result.CheDeng.BaoE,result.CheDeng.BaoFei);

                    //核保成功 //todo test submitStatus = 1;
                    if (submitStatus == "@(Convert.ToInt32(SubmitStatus.Success))"){
                        var channel_ForceCouponRate = @ViewBag.channel_ForceCouponRate;
                        var channel_TaxCouponRate = @ViewBag.channel_TaxCouponRate;
                        var channel_BizCouponRate = @ViewBag.channel_BizCouponRate;

                        var forceRate = 100 - (result.ForceRate + channel_ForceCouponRate);
                        var taxRate = 100 - (result.ForceRate + channel_TaxCouponRate);
                        var bizRate = 100 - (result.BizRate + channel_BizCouponRate);

                        var forceCoupon = result.ForceTotal * (forceRate * 0.01);
                        var taxCoupon = result.TaxTotal * (taxRate * 0.01);
                        var bizCoupon = result.BizTotal * (bizRate * 0.01);

                        var afterCouponTotal = total - (bizCoupon + forceCoupon + taxCoupon);

                        $("#bizCoupon>div:eq(1)").html(bizRate +"%");
                        $("#bizCoupon>div:eq(2)").html(" x " + result.BizTotal + "元（原价）");
                        $("#bizCoupon>div:eq(3)").html(" = "+common.operateNum.formatNum(bizCoupon,2) + "元");

                        $("#forceCoupon>div:eq(1)").html(forceRate+"%");
                        $("#forceCoupon>div:eq(2)").html(" x "+result.ForceTotal + "元（原价）");
                        $("#forceCoupon>div:eq(3)").html(" = "+common.operateNum.formatNum(forceCoupon,2) + "元");

                        $("#taxCoupon>div:eq(1)").html(taxRate+"%");
                        $("#taxCoupon>div:eq(2)").html(" x "+result.TaxTotal + "元（原价）");
                        $("#taxCoupon>div:eq(3)").html(" = "+common.operateNum.formatNum(taxCoupon,2) + "元");

                        var totalCouponHtml = "为您节省："+common.operateNum.formatNum(afterCouponTotal,2)+" 元";
                        $("#totalCoupon").html(totalCouponHtml);

                        var afterCouponTotalHtml = "优惠价后总价："+common.operateNum.formatNum((bizCoupon + forceCoupon + taxCoupon),2)+" 元";
                        $("#afterCouponTotal").html(afterCouponTotalHtml);

                        var totalHtml = common.operateNum.formatNum(total,2) + " 元";
                        $("#totalFooter").html(totalHtml);

                        var afterCouponTotalFooterHtml =common.operateNum.formatNum((bizCoupon + forceCoupon + taxCoupon),2)+ "元";
                        $("#afterCouponTotalFooter").html(afterCouponTotalFooterHtml);


                        //按钮可点击
                        $("#submit").removeClass("t-disable");

                        $(".formula").show();
                        $(".formulaBuy").show();
                    }
                    else if (submitStatus == "@(Convert.ToInt32(SubmitStatus.Checking))"){
                        //新版UI核保中，显示公式部分，且费率显示loading

                        $("#bizCoupon>div:eq(2)").html("x"+result.BizTotal + "元（原价）");
                        $("#forceCoupon>div:eq(2)").html("x"+result.ForceTotal + "元（原价）");
                        $("#taxCoupon>div:eq(2)").html("x"+result.TaxTotal + "元（原价）");

                        $(".formula").show();
                        $(".formulaBuy .t-info").hide();
                        $(".formulaBuy").show();
                    }
                    else{
                        //新版UI核保失败/未核保，不显示公式部分->显示灰色的购买按钮
                        $(".formula").hide();
                        $(".formulaBuy .t-info").hide();
                        $(".formulaBuy").show();
                    }

                    layer.closeAll();
                    //首次报价成功
                    if(result.QuoteStatus > 0 && requestFlag < 1){

                        //标识调用次数
                        requestFlag += 1;
                        //获取核保信息
                        getSubmitInfo();
                    }
                },
                error: function (data) {
                    alert("获取报价信息失败！");
                    return;
                }
            });
        }

        //获取核保信息
        function getSubmitInfo(){
            $.ajax({
                type: "post",
                url: '@Url.Action("GetSubmitInfo", "ProductV3")',
                dataType: "json",
                data: {
                    channel: "@ViewBag.channel",
                    mobile: "@ViewBag.mobile",
                    licenseNo: "@ViewBag.licenseNo",
                    intentionCompany: "@ViewBag.intentionCompany",
                    ProductItemId:"@Request["ProductItemId"]"
                },
                beforeSend: function () {
                    layer.open({ type:2 });
                },
                success: function (data) {
                    console.log(data);
                    if(data == null){
                        alert("获取核保信息失败！");
                        return;
                    }
                    if (data.BusinessStatus != 1) {
                        var message = data.StatusMessage;
                        if (message == null) {
                            message = "获取核保信息失败，服务器未返回失败原因！";
                        }
                        alert(message);
                    }
                    if (data.Item == null) {
                        alert("没有获取到核保信息！");
                        return;
                    }

                    var result = data.Item;
                    var submitStatus = result.SubmitStatus;
                    var submitStatusHtml = "";

                    if (submitStatus == "@(Convert.ToInt32(SubmitStatus.Fail))")
                    {
                        submitStatusHtml =  "<div class='page12-hbzt-title page12-hbzt-error-title'><span>核保<br>失败</span></div>"+
                                            "<div class='page12-hbzt-content page12-hbzt-error-content'><span>原因：<br>"+result.SubmitResult+"<br>核保成功下单可享低至7折优惠</span></div>"+
                                            "<div class='page12-hbzt-error-other'> <a class='page12-hbzt-error-other-button' href='javascript:history.go(-2);'>重新配置</a><br><a class='page12-hbzt-error-other-400' href='javascript:linkTel();'>咨询客服：400-963-1088</a></div>";

                        //var ahtml = linkTelHtml();
                        //"<div class='page12-hbzt-error-other'> <a class='page12-hbzt-error-other-button' href='javascript:history.go(-2);'>重新配置</a><br><a class='page12-hbzt-error-other-400' href='javascript:void(0);'>"+ahtml+"</a></div>";
                    }
                    else if (submitStatus == "@(Convert.ToInt32(SubmitStatus.Success))")
                    {
                        submitStatusHtml = "<div class='page12-hbzt-title page12-hbzt-success-title'><span>核保<br>成功</span></div>"+
                                           "<div class='page12-hbzt-content page12-hbzt-success-content'><span>核保内容："+result.SubmitResult+"</span></div>";
                    }
                    else if (submitStatus == "@(Convert.ToInt32(SubmitStatus.UnCheck))")
                    {
                        submitStatusHtml = "<div class='page12-hbzt-title page12-hbzt-no-title'><span>未核保</span></div>"+
                                           "<div class='page12-hbzt-content page12-hbzt-no-content'><span>亲，保险到期日三月内可以核保算费，届时试算可享低至7折优惠哦！</span></div>" +
                                           "<div class='page12-hbzt-error-other'> <a class='page12-hbzt-error-other-button' href='javascript:backToIndex();'>回到首页</a><br><a class='page12-hbzt-error-other-400' href='javascript:linkTel();'>咨询客服：400-963-1088</a></div>";

                        //var ahtml = linkTelHtml();
                        //"<div class='page12-hbzt-error-other'> <a class='page12-hbzt-error-other-button' href='javascript:backToIndex();'>回到首页</a><br><a class='page12-hbzt-error-other-400' href='javascript:void(0);'>"+ahtml+"</a></div>";
                    }
                    else if (submitStatus == "@(Convert.ToInt32(SubmitStatus.Checking))")
                    {
                        submitStatusHtml = "<div class='page12-hbzt-title page12-hbzt-loading-title'><span>核保中</span></div>"+
                                           "<div class='page12-hbzt-content page12-hbzt-loading-content'><span>请稍等...</span></div>";
                    }

                    $(".page12-hbzt-wrap .page12-hbzt").html(submitStatusHtml);

                    layer.closeAll();

                    getSpecialPrecisePrice(submitStatus);
                },
                error: function (data) {
                    alert("获取核保信息失败！！");

                }
            });
        }

