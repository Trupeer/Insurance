        function showItem(source,value1,value2,sourceId){
            if(value1 > 0){
                source.removeClass("t-loading").text(value2);
                var valueShow = "";
                if(sourceId == "boli"){
                    if(value1 == "1"){
                        valueShow="国产";
                    }
                    else if(value1 == "2"){
                        valueShow = "进口";
                    }
                }
                else{
                    if(value1<10000){
                        valueShow = value1+"元";
                    }
                    else{
                        valueShow = (value1 + ".00").replace("0000.00","万");
                    }
                }
                source.parent(".f-list01-other").siblings(".f-list01-main").find('label').html(valueShow);
                source.parent(".f-list01-other").parent(".f-list01").show();
            }
        }


